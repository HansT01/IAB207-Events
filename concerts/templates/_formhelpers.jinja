{% macro render_field(field) %}

{% if field.type != "SubmitField" and field.type != "HiddenField" %}
{{ field.label }}
{% endif %}

{{ field(**kwargs)|safe }}

{% if field.errors %}
<ul class="errors">
  <p>error here</p>
  {% for error in field.errors %}
  {{flash(error)}}
  {% endfor %}
</ul>
{% endif %}

{% endmacro %}